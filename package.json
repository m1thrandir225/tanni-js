{
  "name": "tanni-js",
  "version": "0.0.1",
  "private": true,
  "description": "A TypeScript-first frontend framework with Vue-like syntax and signal-based reactivity",
  "keywords": [
    "frontend",
    "framework",
    "signals",
    "reactive",
    "typescript"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit --pretty false",
    "build": "pnpm -r run build",
    "dev": "pnpm --filter tannijs run dev"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Sebastijan Zindl",
  "license": "MIT",
  "packageManager": "pnpm@10.30.2+sha512.36cdc707e7b7940a988c9c1ecf88d084f8514b5c3f085f53a2e244c2921d3b2545bc20dd4ebe1fc245feec463bb298aecea7a63ed1f7680b877dc6379d8d0cb4",
  "engines": {
    "node": ">=21.0.0",
    "pnpm": ">=10.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/vite": "^4.2.0",
    "@types/node": "^25.0.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^17.0.0",
    "jiti": "^2.6.1",
    "jsdom": "^28.0.0",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.16"
  }
}
