{
  "name": "tannijs-compiler",
  "version": "0.1.4",
  "type": "module",
  "description": "SFC compiler for the Tanni framework",
  "license": "MIT",
  "author": "Sebastijan Zindl",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean --out-dir dist",
    "prepublishOnly": "pnpm run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
